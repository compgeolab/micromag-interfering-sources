@article{Neel1955,
abstract = {The memoir is devoted to a brief theoretical study of the most typical magnetic properties of rocks. In particular {\S}{\S} 3–16 are on ferrimagnetism, {\S}{\S} 17–35 on single domain particles and {\S}{\S} 36–57 on large multi-domain particles.Theoretical studies are made of the following aspects of the subject and compared with the experimental results: remanent magnetization ({\S} 38), initial susceptibility ({\S} 39), variation with applied field of thermoremanent magnetization (abbreviated to T.R.M.) ({\S}{\S} 40, 41, 57), the ratio Qk of T.R.M. acquired in a given field to the induced magnetization in the same field ({\S} 42), the additivity of partial T.R.M.'s in the case both of small grains ({\S} 28) and large grains ({\S} 57).Considerable space is devoted to the magnetic ‘viscosity' due to thermal agitation in small grains ({\S}{\S} 24–27) and in larger ones ({\S}{\S} 49–56). Expressions are given for magnetic ‘viscosity' in the range of Rayleigh's relations ({\S} 51) particularly with a demagnetizing field present ({\S} 54). The theoretical and experimental results on the irreversible decrease in isothermal remanent magnetization are briefly quoted both for small ({\S} 30) and large ({\S} 55) grains. Different reversing mechanisms are reviewed which could cause a negative T.R.M., that is one directed in the opposite sense to the field applied during cooling. Some are related to negative Weiss-Heisenberg exchange forces: reversal by diffusion involving ionic exchange between the two sub-lattices in a ferrimagnetic ({\S} 7), reversal by anomalous thermal variation is spontaneous magnetization ({\S}{\S} 11, 12), reversal by diffusion with complete change of composition ({\S} 16). The others are effects of the demagnetizing field: reversal in mixtures of two constituents with different Curie Points ({\S}{\S} 31–34), reversal by segregation, allotropy and chemical alteration ({\S} 35). The actual examples so far known are recalled. {\textcopyright} 1955 Taylor & Francis Group, LLC.},
author = {N{\'{e}}el, L.},
doi = {10.1080/00018735500101204},
issn = {14606976},
journal = {Advances in Physics},
number = {14},
pages = {191--243},
publisher = {Taylor & Francis Group},
title = {{Some theoretical aspects of rock-magnetism}},
volume = {4},
year = {1955}
}
